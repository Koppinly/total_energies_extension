    "css": "@media (min-width:975px) {\n  .lg\\:col-span-2 {\n    grid-column: span 2 / span 2;\n  }\n  .lg\\:col-start-2 {\n    grid-column-start: 2;\n  }\n}\n  \n  .cards > a {\n    color: #374649;\n    transition: all .5s ease;\n    text-decoration: none;\n    border: 1px solid #fff;\n  }\n  .cards > a:hover {\n    text-decoration: none;\n    box-shadow: none;\n    border-color: #b7cbd3;\n  }\n  #block-faqmaincontactblock {\n    max-width: 840px;\n    display: flex;\n    flex-direction: column;\n    margin-left: auto;\n    margin-right: auto;\n  }\n  .quickest {\n    position: absolute;\n    display: flex;\n    flex-direction: row;\n    align-items: center;\n    gap: 10px;\n    left: auto;\n    right: 0;\n    top: 60px;\n    background-color: var(--Flat-Yellow, #FFE74F);\n    width: 161px;\n    height 30px;\n    padding: 4px 15px 4px 10px;\n    border-radius: 70px;\n    font-family: Roboto;\n    font-size: 14px;\n    font-style: normal;\n    font-weight: 500;\n    line-height: 22px;\n  }\n  @media (min-width: 485px) {\n    .quickest {\n      position: relative;\n      left: 300px;\n      top: -270px;\n      right: auto;\n      width: 161px;\n      height 40px;\n      padding: 7px 15px 7px 10px;\n      font-size: 16px;\n      line-height: 26px;\n    }\n  }\n  @media (min-width: 768px) {\n    .quickest {\n      position: absolute;\n      top: 60px;\n      right: 0;\n      left: auto;\n    }\n  }\n  @media (min-width: 905px) {\n    .quickest {\n      right: -30px;\n    }\n  }",
